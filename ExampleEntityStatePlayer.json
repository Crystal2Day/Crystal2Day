{
  "name": "Player",

  "description": "Basic player entity derived from another entity",

  "default_state": {
    "test": 12345
  },

  "based_on": {
    "entity_type": "Figure",
    "overwrite_default_state": true
  },

  "coroutine_templates": {
    "update": {"file": "ExampleUpdate.rb"},
    "post_update": {"proc": "FigurePostUpdate"},
    "handle_event": {"code": "loop do; entity.call_proc(\"FigureHandleEvent\"); Fiber.yield; end"}
  }
}
