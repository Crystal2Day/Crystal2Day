{
  "name": "Player",

  "description": "Basic player entity derived from another entity",

  "default_state": {
    "test": 12345
  },

  "based_on": {
    "entity_type": "Figure",
    "overwrite_default_state": true,
    "overwrite_coroutine_templates": true
  },

  "coroutine_templates": {
    "update": {
      "pages": {
        "main": {"file": "example/ExampleUpdatePlayer.rb"},
        "fast": {"file": "example/ExampleUpdatePlayerFast.rb"}
      }
    },
    "post_update": {"proc": "FigurePostUpdate"},
    "handle_event": {"code": "each_frame do; entity.call_proc(\"FigureHandleEvent\"); end"},
    "entity_collisions": {"file": "example/ExampleCollisionEntity.rb"},
    "tile_collisions": {"file": "example/ExampleCollisionTile.rb"}
  }
}
